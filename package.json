{
  "name": "@that-one-tom/n8n-nodes-dmn",
  "version": "0.1.15",
  "description": "n8n node for DMN (Decision Model and Notation) decision tables",
  "keywords": [
    "n8n-community-node-package",
    "dmn",
    "decision-table",
    "rules-engine"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "that-one-tom",
    "email": "thomas@thomasmartens.eu"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/that-one-tom/n8n-nodes-dmn.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && cp -r nodes/icons/ dist/nodes/",
    "dev": "tsc --watch",
    "format": "prettier --write .",
    "lint": "eslint nodes --ext .ts",
    "lintfix": "eslint nodes --ext .ts --fix",
    "prepublishOnly": "npm run build && eslint nodes --ext .ts -c .eslintrc.prepublish.js"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
      "dist/nodes/DMN/Dmn.node.js"
    ]
  },
"devDependencies": {
    "@typescript-eslint/parser": "~8.32.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
