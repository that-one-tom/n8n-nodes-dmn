{
  "name": "n8n-nodes-dmn",
  "version": "0.1.0",
  "description": "n8n node for DMN (Decision Model and Notation) decision tables",
  "keywords": [
    "n8n-community-node-package",
    "dmn",
    "decision-table",
    "rules-engine"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "that-one-tom",
    "email": "thomas@thomasmartens.eu"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/n8n-nodes-dmn.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && cp -r nodes/icons/ dist/nodes/",
    "dev": "tsc --watch",
    "format": "prettier --write .",
    "lint": "eslint nodes --ext .ts",
    "lintfix": "eslint nodes --ext .ts --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
      "dist/nodes/DMN/DMN.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.8.10",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
